<h1>Good Food Hunting</h1>

<ul>
  <li><a href="/">All</a></li>
  <% @dish_types.each do |dish_type| %>
    <li>
      <%= link dish_type.name, "/?dish_type_id=#{dish_type.id}" %>
    </li>
  <% end %>
</ul>


<a href="/dishes/new">new dish</a>

<% @dishes.each do |dish| %>
  <a href="/dishes/<%= dish.id %>"><h2><%= dish.name %></h2></a>

  <img src="<%= dish.image_url %>">
<% end %>

